@model IEnumerable<MusicWeb.Models.User>

@{
    ViewBag.Title = "DSUser";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>DANH SÁCH USER</h2>

<table class="table">
    <tr>
        <th>
            Mã User:
        </th>
        <th>
            Tên User:
        </th>
        <th>
            Tài Khoản:
        </th>
        <th>
            Mật Khẩu:
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaUser)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TenUser)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TaiKhoan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MatKhau)
        </td>
        <td>
            @Html.ActionLink("Delete", "XoaUser", new { id=item.MaUser })
        </td>
    </tr>
}

</table>
